        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HighGroundWpf"
        mc:Ignorable="d"
        Title="Window1" Height="300" Width="300" WindowStyle="None" WindowStartupLocation="CenterOwner" Topmost="True" ResizeMode="NoResize" MouseDown="Window_MouseDown">
    <Grid HorizontalAlignment="Center" VerticalAlignment="Center" Height="270" Margin="0,0,0.333,0.667">
        <Label Content="All Done!" Margin="0,67,0,129" FontSize="48" RenderTransformOrigin="0.493,-0.26" HorizontalAlignment="Center" VerticalAlignment="Center"/>
        <Button x:Name="Directions_Button" Content="Get Directions" Height="52" Margin="0,177,0,0" VerticalAlignment="Top" FontSize="20" BorderBrush="White" Background="#FF99DFF9" Click="Directions_Button_Click"/>
        <Button x:Name="Close" Content="X" Height="31" Margin="216,-12,-43.667,0" VerticalAlignment="Top" Background="#FFF799D1" Click="Close_Click"/>

    </Grid>
</Window>
